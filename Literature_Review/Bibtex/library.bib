@article{Cresci2010,
abstract = {It has recently been suggested that galaxies in the early Universe could have grown through the accretion of cold gas, and that this may have been the main driver of star formation and stellar mass growth. Because the cold gas is essentially primordial, it has a very low abundance of elements heavier than helium (referred to as metallicity). If funnelled to the centre of a galaxy, it will result in the central gas having an overall lower metallicity than gas further from the centre, because the gas further out has been enriched by supernovae and stellar winds, and not diluted by the primordial gas. Here we report chemical abundances across three rotationally supported star-forming galaxies at redshift z ≈ 3, only 2 Gyr after the Big Bang. We find 'inverse' gradients, with the central, star-forming regions having lower metallicities than less active ones, which is opposite to what is seen in local galaxies. We conclude that the central gas has been diluted by the accretion of primordial gas, as predicted by 'cold flow' models.},
author = {Cresci, G and Mannucci, F and Maiolino, R and Marconi, a and Gnerucci, a and Magrini, L},
doi = {10.1038/nature09451},
file = {:Users/owenturner/Documents/PhD/KMOS/Literature/Cresci_2010 Gas Metallicity maps.pdf:pdf},
issn = {1476-4687},
journal = {Nature},
month = oct,
number = {7317},
pages = {811--3},
pmid = {20944741},
publisher = {Nature Publishing Group},
title = {{Gas accretion as the origin of chemical abundance gradients in distant galaxies.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/20944741},
volume = {467},
year = {2010}
}
