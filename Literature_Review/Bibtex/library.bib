title = {{The Cosmic BPT Diagram: Confronting Theory with Observations}},
url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2013arXiv1307.0514K&link_type=ABSTRACT\npapers://939e88c8-a723-4abe-bb43-ccd202439e84/Paper/p25967 http://iopscience.iop.org/2041-8205/774/1/L10},
